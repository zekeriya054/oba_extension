{
    "name": "OBA otomatik video izleme",
    "description": "OBA videolarını arkaplanda oynatır ve otomatik geçiş yapar",
    "version": "1.0",
    "manifest_version": 3,

    "icons": {
        "16": "./icons/icon-16x16.png",
        "36": "./icons/icon-36x36.png",
        "48": "./icons/icon-48x48.png",
        "120": "./icons/icon-120x120.png"
    },
    "content_scripts": [
        {
            "matches": [
                
                "https://*.oba.gov.tr/*"
      
            ],
            "js": ["content-script.js"]
        }
    ],

    "action": {}

}
